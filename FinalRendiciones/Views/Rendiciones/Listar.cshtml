@using FinalRendiciones.Models
@{
    ViewBag.Title = "Listar";
}

<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>Listar</title>
    <meta charset="utf-8" />
</head>
<body>

    <h1>Listar</h1>
    <table border="1">
        <tr><th>ID</th><th>Nombre</th><th>Concepto</th><th>Importe</th></tr>

        @foreach(ViaticoDto V in Model)
        {
            <tr><td>@V.IdViatico</td><td>@V.Nombre</td><td>@V.Concepto</td><td>@V.Importe</td></tr>
        }
    </table>

    @Html.ActionLink("Volver", "Index")
</body>
</html>
